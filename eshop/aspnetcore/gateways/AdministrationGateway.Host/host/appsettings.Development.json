{
  "App": {
    // [consul:http://localhost:8500],[vue_Administration:http://localhost:4200]
    "CorsOrigins": "https://*.AdministrationGateway.com,http://localhost:4200,http://localhost:8500"
  },
  "AuthServer": {
    "ApiName": "EShop.AdministrationGateway",
    "Authority": "https://localhost:44115",
    "RequireHttpsMetadata": "true",

    "SwaggerClientId": "EShop.AdministrationGateway.Swagger",
    "SwaggerClientScopes": [ "EShop.AdministrationGateway.Swagger" ]
  },
  "ServiceDiscovery": {
    "ServiceName": "EShop.AdministrationGateway",
    "Tags": [ "abp-vue-vben", "gateway" ],
    "Consul": {
      "HttpEndpoint": "http://localhost:8500"
    },
    "Endpoints": [
      "https://localhost:44805"
    ],
    "HealthCheck": {
      "HealthCheckTemplate": "/api/health",
      "Interval": 10
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}
