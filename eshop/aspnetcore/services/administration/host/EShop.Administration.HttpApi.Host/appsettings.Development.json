{
  "App": {
    "SelfUrl": "https://localhost:44145",
    // [consul:http://localhost:8500],[vue_Administration:http://localhost:4200]
    // [AdministrationGateway:https://localhost:44805]
    "CorsOrigins": "https://*.Administration.com,http://localhost:4200,http://localhost:8500,https://localhost:44805"
  },
  "Application": {
    "AppName": "EShop.Administration.Service",
    "Auth": {
      "Authority": "https://localhost:44115",
      "RequireHttpsMetadata": "true"
    },
    "Swagger": {
      "ClientId": "EShop.Administration.Service.Swagger",
      "ClientSecret": ""
    }
  },
  "ServiceDiscovery": {
    "ServiceName": "EShop.Administration.Service",
    "Tags": [ "abp-vue-vben", "api" ],
    "Consul": {
      "HttpEndpoint": "http://localhost:8500"
    },
    "Endpoints": [
      "https://localhost:44145"
    ],
    "HealthCheck": {
      "HealthCheckTemplate": "/api/health",
      "Interval": 10
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Leopard": {
    "Log": {
      "EnableToFile": true,
      "EnableToElasticsearch": false
    }
  }
}
