{
  "App": {
    "SelfUrl": "https://localhost:44145",
    // [consul:http://localhost:8500],[vue_Administration:http://localhost:4200]
    // [AdministrationAppGateway:https://localhost:44805]
    "CorsOrigins": "https://*.Administration.com,http://localhost:4200,http://localhost:8500,https://localhost:44805"
  },
  "AuthServer": {
    "ApiName": "EShop.Administration.Service",
    "Authority": "https://localhost:44115",
    "RequireHttpsMetadata": "true",

    "SwaggerClientId": "EShop.Administration.Service.Swagger",
    "SwaggerClientSecret": "",
    // 如果范围只有自己本身，那么不能放空，要把自己的client标识放进去，不然验证会报“无效的范围”
    "SwaggerClientScopes": [ "EShop.Administration.Service.Swagger" ]
  },
  "ServiceDiscovery": {
    "ServiceName": "EShop.Administration.Service",
    "Tags": [ "abp-vue-vben", "api" ],
    "Consul": {
      "HttpEndpoint": "http://localhost:8500"
    },
    "Endpoints": [
      "https://localhost:44145"
    ],
    "HealthCheck": {
      "HealthCheckTemplate": "/api/health",
      "Interval": 10
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}
